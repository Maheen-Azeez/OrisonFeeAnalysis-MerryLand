    public async Task<double> GetRowCounterFee(int? val)
    {
        var index = await GvFeeStatus.GetRowIndexByPrimaryKeyAsync(val);
        var currentPage = GvFeeStatus.PageSettings.CurrentPage;
        if (GvFeeStatus.AllowPaging && currentPage > 1)
        {
            index = (((currentPage - 1) * GvFeeStatus.PageSettings.PageSize) + index);
        }
        return index + 1;

    }
