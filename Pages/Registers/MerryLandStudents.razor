    public async Task init_edit()
    {
        try
        {
            DTAccount = await StudentMasterService.GetDTAccount(AccountID);
            objParentID = DTAccount.Parent;

            DTStudent = await StudentMasterService.GetDTStudent(AccountID);
            DTStudent.ProcessDate = Convert.ToDateTime(DTStudent.ProcessedDate);
            //ProcessDate= Convert.ToDateTime(DTStudent.ProcessedDate).ToString("dd/MM/yyyy hh:mm:ss tt", CultureInfo.InvariantCulture);
            if (DTStudent.DateOfBirth != null)
                Age = await StudentMasterService.GetAge(AccountID);

            if (DTStudent.MinistryStatus == "EXEMPTION")
                await JsRuntime.InvokeVoidAsync("onColor", "StudCategoryID", "Green");
            else
                await JsRuntime.InvokeVoidAsync("onColor", "StudCategoryID", "Black");


            DTStudentTrans = await StudentMasterService.GetDTStudentTrans(AccountID, BranchID, academicYear);
            if (DTStudentTrans.Status == "STUDYING")
                await JsRuntime.InvokeVoidAsync("onColor", "StatusID", "Green");
            else
                await JsRuntime.InvokeVoidAsync("onColor", "StatusID", "Red");

            if (DTStudentTrans.Bus_No != null)
            {
                // SelectedBusChanged();
            }
            if (DTStudentTrans.Class != null && DTStudentTrans.Division != null)
            {
                TeacherName = await StudentMasterService.GetTeacherName(DTStudentTrans.Class, DTStudentTrans.Division, BranchID);
            }

            DTParent = await StudentMasterService.GetDTParent(Convert.ToInt32(objParentID));
            objStaffParent = DTParent.StaffParent;
            if (DTParent.PrimaryContact != null)
                if (DTParent.PrimaryContact == "Father")
                    FPrimary = "Primary";
                else if (DTParent.PrimaryContact == "Mother")
                    MPrimary = "Primary";

            DTParentAccount = await StudentMasterService.GetDTAccount(Convert.ToInt32(objParentID));

            if (objStaffParent != null)
            {
                DTStaff = await StudentMasterService.GetDTAccount(Convert.ToInt32(objStaffParent));
                DTStaff.AccountName = DTStaff.AccountName;
            }
            else
            {
                DTStaff.AccountCode = null;
                DTStaff.AccountName = "";
            }

            DTStudentsArabic = await StudentMasterService.GetDTStudentsArabic(AccountID);

            DTImages = await StudentMasterService.GetDTStudentImage(AccountID);

            if (DTImages.Photo == null)
            {

                StudentImage = "data:image/png;base64,";
            }
            else
            {
                MemoryStream ms = new MemoryStream(DTImages.Photo);
                string base64String = Convert.ToBase64String(DTImages.Photo, 0, DTImages.Photo.Length);
                StudentImage = "data:image/png;base64," + base64String;
            }

            if (DTParent.FatherPhoto == null)
            {

                FatherImage = "data:image/png;base64,";
            }
            else
            {
                MemoryStream ms = new MemoryStream(DTParent.FatherPhoto);
                string base64String = Convert.ToBase64String(DTParent.FatherPhoto, 0, DTParent.FatherPhoto.Length);
                FatherImage = "data:image/png;base64," + base64String;
            }

            if (DTParent.MotherPhoto == null)
            {

                MotherImage = "data:image/png;base64,";
            }
            else
            {
                MemoryStream ms = new MemoryStream(DTParent.MotherPhoto);
                string base64String = Convert.ToBase64String(DTParent.MotherPhoto, 0, DTParent.MotherPhoto.Length);
                MotherImage = "data:image/png;base64," + base64String;
            }

            if (DTParent.ThirdContactPhoto == null)
            {

                ThirdImage = "data:image/png;base64,";
            }
            else
            {
                MemoryStream ms = new MemoryStream(DTParent.ThirdContactPhoto);
                string base64String = Convert.ToBase64String(DTParent.ThirdContactPhoto, 0, DTParent.ThirdContactPhoto.Length);
                ThirdImage = "data:image/png;base64," + base64String;
            }

            StudDocuments = (await StudentMasterService.GetStudDocumentById(AccountID)).ToList();
            StudAdditionalPayment = (await StudentMasterService.GetAdditionalPayment(AccountID)).ToList();
            StudAdditionalDiscount = (await StudentMasterService.GetAdditionalDiscount(AccountID)).ToList();
            StudRelation = (await StudentMasterService.GetRelation(DTParentAccount.Id)).ToList();
            StudNotes = (await StudentMasterService.GetNotes(AccountID)).ToList();
            SiblingRegister = (await StudentMasterService.GetSiblingRegister(Convert.ToInt32(objParentID), BranchID, AccountID, academicYear)).ToList();
            //await InvokeAsync(StateHasChanged);
        }
        catch (Exception e)
        {

            throw e;
        }

    }


