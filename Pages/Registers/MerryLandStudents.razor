        predicate.Add(new WhereFilter() { Condition = "or", Field = "Mobile", value = args.Text, Operator = "contains", IgnoreAccent = true, IgnoreCase = true });
        pre = WhereFilter.Or(predicate);
        var query = new Query().Where(pre).Take(40);
        await this.SfStudentObj.Filter(StudentList, query);
        await InvokeAsync(StateHasChanged);
    }

