    private async Task OpenNewParent()
    {
        object ParentCodeFromFunctionWithBranch = (await StudentMasterService.BindSettingsValue("ParentCodeFromFunctionWithBranch"));
        if (Convert.ToBoolean(ParentCodeFromFunctionWithBranch) == true)
        {
            object ParentCodeSameAsStudentCode = (await StudentMasterService.BindSettingsValue("ParentCodeSameAsStudentCode"));
            if (Convert.ToBoolean(ParentCodeSameAsStudentCode) == false || ParentCodeSameAsStudentCode.ToString() != "")
            {
                Abbr = (await StudentMasterService.GetAbbr(BranchID)).ToString();
                string SExistCode = await StudentMasterService.GetSExistNo(BranchID);
                if (SExistCode == null)
                {
                    StudentCode = Abbr + "S01001";
                    ParentCode = Abbr + "P01001";
                }
                else
                {
                    StudentCode = await StudentMasterService.GetNextNo(BranchID);
                }

                string PExistCode = await StudentMasterService.GetParentNextNo(BranchID, StudentCode);
                if (PExistCode == null)
                {

                }
                else
                {
                    ParentCode = PExistCode.ToString();
                }
            }
            else
            {
                string SExistCode = await StudentMasterService.GetSExistNo(BranchID);
                if (SExistCode == null)
                {
                    StudentCode = Abbr + "S01001";
                    ParentCode = Abbr + "P01001";
                }
                else
                {
                    StudentCode = Abbr + "S" + SExistCode;   //await StudentMasterService.GetNextNo(BranchID);
                    ParentCode = Abbr + "P" + SExistCode;
                }
            }
        }
        else
        {
            string SExistCode = await StudentMasterService.GetSExistNo(BranchID);
            if (SExistCode == null)
            {
                StudentCode = "S01001";
                ParentCode = "P01001";
            }
            else
            {
                StudentCode = "S" + SExistCode;   //await StudentMasterService.GetNextNo(BranchID);
                ParentCode = "P" + SExistCode;
            }
        }


        DTNewParentAccount.AccountCode = ParentCode;
        this.PopupVisible = true;
    }

